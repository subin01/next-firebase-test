{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "build": "next build && next export",
    "github_build:dev": "env-cmd -f ./.env.development next build && next export",
    "github_build:prod": "env-cmd -f .env.production next build && next export",
    "build:dev": "env-cmd -f .env.development next build && next export",
    "build:prod": "env-cmd -f .env.production next build && next export",
    "deploy:dev": "env-cmd -f .env.development npm run build && firebase deploy --only hosting --project dev",
    "deploy:prod": "env-cmd -f .env.production npm run build && firebase deploy --only hosting --project prod"
  },
  "dependencies": {
    "firebase": "^9.9.2",
    "next": "12.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-firebase-hooks": "^5.0.3"
  },
  "devDependencies": {
    "@types/node": "18.7.1",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "env-cmd": "^10.1.0",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.4",
    "typescript": "4.7.4"
  }
}